unit ParentsScreen;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.Grids, Vcl.StdCtrls;

type
  TForm6 = class(TForm)
    PupilName: TLabel;
    ParentName1: TLabel;
    ParentName2: TLabel;
    DnevnikStringGrid1: TStringGrid;
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form6: TForm6;

implementation

{$R *.dfm}

procedure TForm6.FormCreate(Sender: TObject);
begin
  DnevnikStringGrid1.Cells[0,0] := 'Понедельник';
  DnevnikStringGrid1.Cells[0,8] := 'Вторник';
  DnevnikStringGrid1.Cells[0,16] := 'Среда';
end;

end.
